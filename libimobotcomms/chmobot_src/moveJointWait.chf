int CMobot::moveJointWait(int id) {
    void *fptr;
    int retval;

    fptr = dlsym(_Chchmobot_handle, "moveJointWait_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, moveJointWait, this, id);
    return retval;
}
