int CMobot::moveJointContinuousTime(int id, int dir, int msecs) {
    void *fptr;
    int retval;

    fptr = dlsym(_Chchmobot_handle, "moveJointContinuousTime_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, moveJointContinuousTime, this, id, dir, msecs);
    return retval;
}
