int CMobot::connect() {
    void *fptr;
    int retval;

    fptr = dlsym(_Chchmobot_handle, "connect_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, this, connect);
    return retval;
}
