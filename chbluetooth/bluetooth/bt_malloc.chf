void * bt_malloc(size_t size) {
    void *fptr;
    void *retval;

    fptr = dlsym(_Chbluetooth_handle, "bt_malloc_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return NULL;
    }
    dlrunfun(fptr, &retval, bt_malloc, size);
    return retval;
}
