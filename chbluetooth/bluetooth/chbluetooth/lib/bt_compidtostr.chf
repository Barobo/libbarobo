char * bt_compidtostr(int id) {
    void *fptr;
    char *retval;

    fptr = dlsym(_Chbluetooth_handle, "bt_compidtostr_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return NULL;
    }
    dlrunfun(fptr, &retval, bt_compidtostr, id);
    return retval;
}
