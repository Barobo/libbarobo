int bt_error(uint16_t code) {
    void *fptr;
    int retval;

    fptr = dlsym(_Chbluetooth_handle, "bt_error_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return -1;
    }
    dlrunfun(fptr, &retval, bt_error, code);
    return retval;
}
