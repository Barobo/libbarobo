bdaddr_t * strtoba(char *str) {
    void *fptr;
    bdaddr_t *retval;

    fptr = dlsym(_Chbluetooth_handle, "strtoba_chdl");
    if(fptr == NULL) {
        fprintf(_stderr, "Error: %s(): dlsym(): %s\n", __func__,  dlerror());
        return NULL;
    }
    dlrunfun(fptr, &retval, strtoba, str);
    return retval;
}
